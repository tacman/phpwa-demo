{
    "name": "spomky-labs/phpwa-demo",
    "description": "A Symfony based project to demonstrate the use of PHPWA",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "api-platform/core": "^4.0",
        "elao/json-http-form-bundle": "^3.1",
        "knplabs/knp-time-bundle": "^2.2",
        "doctrine/dbal": "^3|^4",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "nelmio/cors-bundle": "^2.5",
        "nelmio/security-bundle": "^3.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^2.0",
        "runtime/frankenphp-symfony": "^0.2.0",
        "spomky-labs/pwa-bundle": "1.3.x@dev",
        "survos/core-bundle": "*",
        "survos/deployment-bundle": "^1.5",
        "symfony/asset": "^7.2",
        "symfony/asset-mapper": "^7.2",
        "symfony/console": "^7.2",
        "symfony/dependency-injection": "^7.2",
        "symfony/dotenv": "^7.2",
        "symfony/expression-language": "^7.2",
        "symfony/flex": "^2",
        "symfony/form": "^7.2",
        "symfony/framework-bundle": "^7.2",
        "symfony/http-foundation": "^7.2",
        "symfony/http-kernel": "^7.2",
        "symfony/intl": "^7.2",
        "symfony/mime": "^7.2",
        "symfony/monolog-bundle": "^3.0",
        "symfony/process": "^7.2",
        "symfony/property-access": "^7.2",
        "symfony/property-info": "^7.2",
        "symfony/routing": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/security-bundle": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/stimulus-bundle": "^2.13",
        "symfony/string": "^7.1",
        "symfony/translation": "^7.1",
        "symfony/twig-bundle": "^7.1",
        "symfony/uid": "^7.1",
        "symfony/ux-icons": "^2.22",
        "symfony/ux-leaflet-map": "^2.22",
        "symfony/ux-live-component": "^2.22",
        "symfony/ux-map": "^2.22",
        "symfony/ux-twig-component": "^2.22",
        "symfony/validator": "^7.2",
        "symfony/web-link": "^7.2",
        "symfony/yaml": "^7.2",
        "symfonycasts/tailwind-bundle": "^0.9",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.1",
            "docker": true
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "dbrekelmans/bdi": "^1.1",
        "ekino/phpstan-banned-code": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-beberlei-assert": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.0",
        "shipmonk/composer-dependency-analyser": "^1.8",
        "shipmonk/dead-code-detector": "^0.10",
        "staabm/phpstan-todo-by": "^0.2.0",
        "struggle-for-php/sfp-phpstan-psr-log": "^0.23.0",
        "symfony/browser-kit": "^7.2",
        "symfony/css-selector": "^7.2",
        "symfony/debug-bundle": "^7.2",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "^2.1",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "^7.1",
        "symfony/web-profiler-bundle": "^7.1"
    }
}
